{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationDir": "./temp/tsc/types",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "importHelpers": false,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "./temp/tsc/js",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "stripInternal": true,
    /*
     * Since this package is released in 2020.
     * See: https://github.com/glyph-cat/react-relink/releases/tag/0.0.1
     */
    "target": "ES2020",
  },
  "exclude": [
    "node_modules",
    "./src/**/*.draft*",
    "./src/**/*.old*",
    "./src/**/*.test*",
  ],
  "include": [
    "src",
  ],
}